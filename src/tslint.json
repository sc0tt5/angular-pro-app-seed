{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            false,
            "attribute",
            [
                "app",
                "auth",
                "example",
                "credit",
                "stock",
                "mail",
                "side",
                "pizza",
                "drink"
            ],
            "kebab-case"
        ],
        "component-selector": [
            true,
            "element",
            [
                "app",
                "auth",
                "example",
                "credit",
                "stock",
                "mail",
                "side",
                "pizza",
                "drink"
            ],
            "kebab-case"
        ],
        "no-inferrable-types": false
    }
}
