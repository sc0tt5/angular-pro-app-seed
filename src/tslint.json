{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            false,
            "attribute",
            ["app", "auth", "health", "meal", "meals", "schedule", "workouts"],
            "kebab-case"
        ],
        "component-selector": [
            true,
            "element",
            ["app", "auth", "health", "meal", "meals", "schedule", "workouts"],
            "kebab-case"
        ],
        "no-inferrable-types": false,
        "ban-types": false
    }
}
