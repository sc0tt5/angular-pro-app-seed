{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            false,
            "attribute",
            ["app", "auth", "example", "credit", "stock", "mail"],
            "kebab-case"
        ],
        "component-selector": [
            true,
            "element",
            ["app", "auth", "example", "credit", "stock", "mail"],
            "kebab-case"
        ],
        "no-inferrable-types": false
    }
}
